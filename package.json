{
  "name": "medium-zoom",
  "version": "0.4.0",
  "description": "Medium zoom on your images in vanilla JavaScript",
  "license": "MIT",
  "repository": "francoischalifour/medium-zoom",
  "author": {
    "name": "Francois Chalifour",
    "email": "francois.chalifour@gmail.com",
    "url": "https://francoischalifour.com"
  },
  "main": "dist/medium-zoom.min.js",
  "module": "dist/medium-zoom.es.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "yarn run lint --fix",
    "format": "prettier --write 'src/**/*.{js,json,css}' && yarn run lint --fix",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "clean": "rimraf ./dist",
    "prebuild": "yarn run test && yarn run clean",
    "build": "cross-env NODE_ENV=build rollup --config",
    "dev": "cross-env NODE_ENV=build rollup --config --watch",
    "preview": "yarn run build && cp ./dist/* ./examples/demo/preview/",
    "prepublishOnly": "yarn run build"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "standard": {
    "ignore": [
      "/dist/",
      "/examples/"
    ],
    "globals": [
      "CustomEvent",
      "HTMLCollection",
      "NodeList",
      "requestAnimationFrame"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "medium",
    "image",
    "zoom",
    "picture",
    "lightbox",
    "click",
    "scroll",
    "pure",
    "plain",
    "vanilla",
    "javascript",
    "ux",
    "performance",
    "event",
    "medium-zoom",
    "zoomable"
  ],
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-env": "1.7.0",
    "cross-env": "5.2.0",
    "cssnano": "4.0.4",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "2.9.0",
    "jest": "23.4.2",
    "prettier": "1.13.7",
    "rimraf": "2.6.2",
    "rollup": "0.63.4",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-babel-minify": "5.0.0",
    "rollup-plugin-commonjs": "9.1.4",
    "rollup-plugin-postcss": "1.6.2",
    "rollup-plugin-uglify": "4.0.0",
    "rollup-watch": "4.3.1"
  }
}
